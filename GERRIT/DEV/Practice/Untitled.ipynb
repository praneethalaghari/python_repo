   "source": [
    "import time\n",
    "\n",
    "def decorate_print(func):\n",
    "    def inner_func(*args,**kwargs):\n",
    "        print(time.time())\n",
    "        func(*args,**kwargs)\n",
    "        print(time.time())\n",
    "    return inner_func\n",
    "\n",
    "@decorate_print\n",
    "def print_():\n",
    "    print(\"Hello Praneeth!\")\n",
    "\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    print_()"
   ]
  }
